try:
    score = float(input("Enter the number :"))
    if((0.0<=score) and (1.0 >= score)):
            if(score>=0.9 and score <= 1.0):
                print("A")
            elif(score>=0.8):
                print("B")
            elif(score>=0.7):
                print("C")
            elif(score>=0.6):
                print("D")
            elif(score>=0.5):
                print("E")
            else:
                print("F")
    else:
        print("out of range")
except:
    print("error in code")
