try:
    z=input("enter score")
    score = float(z)
    if(0.0<=score):
        if(score<=1.0):
            if(score>=0.9):
                printf("A")
            elif(score>=0.8):
                print("B")
            elif(score>=0.7):
                printf("C")
            elif(score>=0.6):
                print("D")
            elif(score>=0.5):
                printf("E")
            else:
                print("F")
    else:
        printf("out of range")
except:
    print("error in code")
